#!/home/user/learning/Python/linuxacademy/code/pgbackup/.tox/py27/bin/python2.7
# EASY-INSTALL-ENTRY-SCRIPT: 'pgbackup','console_scripts','pgbackup'
__requires__ = 'pgbackup'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pgbackup', 'console_scripts', 'pgbackup')()
    )
